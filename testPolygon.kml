<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.1">

<Document>
	<name>Test Polygon</name>
	<description>Trying to see if the maps API takes the kml file in the first place</description>
	
	<Style id="blueBox">
		<LineStyle>
			<color>ffff0000</color>
			<width>2</width>
		</LineStyle>
	</Style>
		
	<Placemark>
		<name>Blue Polygon</name>
		<styleUrl>#blueBox</styleUrl>
		<LineString>
			<altitudeMode>relative</altitudeMode>
				<coordinates>
-80.0,40.0,0
-80.0,41.0,0
-81.0,41.0,0
-81.0,40.0,0
				</coordinates>
		</LineString>
	</Placemark>
	
</Document>
</kml>